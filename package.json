{
  "name": "robabby.com",
  "version": "1.0.0",
  "description": "My website",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server:dev": "nodemon server.js",
    "server:prod": "pm2 start ecosystem.json --env production",
    "client:serve": "npm run start --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:serve\"",
    "prod": "concurrently \"npm run server:prod\" \"npm run client:build\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
